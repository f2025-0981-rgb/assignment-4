#include <iostream>
#include <vector>
#include <string>
using namespace std;

struct Employee {
    string name;
    int age;
    vector<string> skills;
};

int main() {
    int n;
    cout << "Enter number of employees: ";
    cin >> n;

    vector<Employee> employees;

    for (int i = 0; i < n; i++) {
        Employee e;
        int skillCount;

        cout << "\nEnter name of employee " << i + 1 << ": ";
        cin.ignore();
        getline(cin, e.name);

        cout << "Enter age: ";
        cin >> e.age;

        cout << "Enter number of skills: ";
        cin >> skillCount;

        e.skills.resize(skillCount);
        cout << "Enter skills:\n";
        cin.ignore();
        for (int j = 0; j < skillCount; j++) {
            getline(cin, e.skills[j]);
        }

        employees.push_back(e);
    }

    cout << "\nEmployee Details:\n";
    for (int i = 0; i < employees.size(); i++) {
        cout << "\nEmployee " << i + 1 << endl;
        cout << "Name: " << employees[i].name << endl;
        cout << "Age: " << employees[i].age << endl;
        cout << "Skills: ";
        for (string skill : employees[i].skills) {
            cout << skill << " ";
        }
        cout << endl;
    }

    return 0;
}
