// Online C++ compiler to run C++ program online
#include <iostream>
#include<vector>
using namespace std;
struct k
{
    string name;
    string roll;
    string sub;
    int age;
};
void print(k a){
   cout<<"information"<<endl;
      cout<<"name: "<<a.name<<endl;
      cout<<"roll no: "<<a.roll<<endl;
      cout<<"subject: "<<a.sub<<endl;
      cout<<"age: "<<a.age<<endl;
       cout<<endl;
}
k inputt(){
    k a;
   cout<<"enter name :";
   cin>>a.name;
   cout<<"enter roll no :";
    cin>>a.roll;
     cout<<"enter subject :";
      cin>>a.sub;
      cout<<"enter age :";
       cin>>a.age;
       return a;
}

int main() {
    vector<k> vec;
    for(int i=0; i<3;i++){
        k a= inputt();
        vec.push_back(a);
    }
    for(k a:vec){
        print(a);
    }
    return 0;
}
